@model List<TeamGetVM>
<div class="container">
	<div class="row">
		<div class="col-2">
			<a class="btn btn-success" asp-action="Create">Create</a>
		</div>
		<table class="table table-striped">
			<thead>
				<tr>
					<th scope="col">Id</th>
					<th scope="col">FullName</th>
					<th scope="col">Position</th>
					<th scope="col">Image</th>
					<th scope="col">SocialMedia</th>
					<th scope="col">Actions</th>
				</tr>
			</thead>
			<tbody>
				@foreach (var team in Model)
				{
					<tr>
						<th scope="row">@team.Id</th>
						<td>@team.FullName</td>
						<td>@team.Position</td>
						<td><img src="~/img/@team.Image" /></td>
						<td>@team.SocialMedia</td>
						<td>
							<a asp-action="Update" asp-route-id="@team.Id" class="btn btn-warning">Update</a>
							<a asp-action="Delete" asp-route-id="@team.Id" class="btn btn-danger">Delete</a>
						</td>
					</tr>
				}
			</tbody>
		</table>
	</div>
</div>